
page,.manage_view{
    width: 100%;
    height: 100%;
}
.input_view{
    width: 100%;
    height: 70rpx;
    background: #f5f5f5;
    padding: 0 30rpx;
    box-sizing: border-box;
    position: fixed;
    left: 0;
    top: 0;

    z-index: 99;
}
.input_pla{
    font-size: 24rpx;
    color: #bbbbbb;
}
.input_default{
    width: 100%;
    text-align: left;
    height: 38rpx;
    line-height: 38rpx;
    margin: auto 0;
    font-size: 24rpx;
    color: #101d36;
}
.search_icon{
    width: 26rpx;
    height: 26rpx;
    -webkit-flex-shrink: 0;
            flex-shrink: 0;
    margin: auto 13rpx auto 0;
    position: relative;
    top: -2rpx;
}
.cha_icon_view{
    width: 72rpx;
    height: 100%;
    -webkit-flex-shrink: 0;
            flex-shrink: 0;
    margin-right: -25rpx;
}
.cha_icon{
    width: 34rpx;
    height: 34rpx;
    margin: auto;
}
.scroll_view{
    width: 100%;
    height: 100%;
    padding-left: 30rpx;
    box-sizing: border-box;
    padding-top: 70rpx;
}
.list_item{
    padding-top: 20rpx;
    background: #ffffff;
    box-sizing: border-box;
}
.header_view{
    margin: 0 26rpx auto 0;
    -webkit-flex-shrink: 0;
            flex-shrink: 0;
    width: 80rpx;
    height: 80rpx;
    border-radius: 100%;
    overflow: hidden;
}
.header_view>image{
    width: 100%;
    height: 100%;
}
.list_cont{
    height: 104rpx;
    border-bottom: 1rpx solid #f5f5f5;
    box-sizing: border-box;
    width: 100%;
}
.name_view{
    padding-top: 4rpx;
    width: 100%;
    box-sizing: border-box;
}
.dept_name{
    height: 40rpx;
    line-height: 40rpx;
    text-align: left;
    font-size: 30rpx;
    color: #333333;
}
.name{
    height: 30rpx;
    font-size: 20rpx;
    color: #999999;
    line-height: 30rpx;
    margin-bottom: 30rpx;
}
.info_right{
    -webkit-flex-shrink: 0;
            flex-shrink: 0;
    height: 48rpx;
    line-height: 48rpx;
    margin: 20rpx 0 auto 0;
}
.num{
    width: 90rpx;
    text-align: center;
    font-size: 28rpx;
    color: #666666;
}
.icon_view{
    width: 90rpx;
    height: 48rpx;
    border-left: 1rpx solid #f5f5f5;
    box-sizing: border-box;
}
.chart_icon{
    width: 44rpx;
    height: 42rpx;
    -webkit-flex-shrink: 0;
            flex-shrink: 0;
    margin: auto;
}
.tel_icon{
    width: 42rpx;
    height: 42rpx;
    -webkit-flex-shrink: 0;
            flex-shrink: 0;
    margin: auto;
}
.avatar_empty{
    width: 100%;
    height: 100%;
    background: #3396fb;
    text-align: center;
    line-height: 80rpx;
    color: #ffffff;
    font-size: 36rpx;
}

