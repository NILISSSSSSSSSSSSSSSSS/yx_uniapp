/*状态搜索相关样式 S*/
.search_box{
    width: 100%;
    background: #FFFFFF;
    z-index: 99;
}
.search_source_list{
    position: fixed;
    top: 89rpx;
    left: 0;
    overflow-y: auto;
    width: 100%;
    background: rgba(0,0,0,0.4);
    height: 100%;
    z-index: 88;
}
.search_header{
    border-bottom: solid 1px #F5F5F5;
}
.source_of_search{
    margin-right: 260rpx;
}
.search_text{
    font-size: 28rpx;
    line-height: 90rpx;
}
.search_text_click{
    color: #0084ff;
}
.search_icon_xia{
    width: 14rpx;
    height: 8rpx;
    opacity: 0.3;
    margin: auto auto auto 15rpx;
}
.search_icon_shang{
    width: 14rpx;
    height: 8rpx;
    margin: auto auto auto 15rpx;
}
.menu{
    -webkit-animation: menu .2s ease-in-out forwards;
            animation: menu .2s ease-in-out forwards;
}
@-webkit-keyframes menu {
0% {
        top: -400px;
}
50% {
        top: -200px;
}
100% {
        top: 0;
}
}
@keyframes menu {
0% {
        top: -400px;
}
50% {
        top: -200px;
}
100% {
        top: 0;
}
}
@-webkit-keyframes showMark{
0%{-webkit-transform: scale(1);transform: scale(1); background: rgba(0,0,0,0);}
100%{-webkit-transform: scale(1);transform: scale(1); background: rgba(0,0,0,0.3);}
}
@keyframes showMark{
0%{-webkit-transform: scale(1);transform: scale(1); background: rgba(0,0,0,0);}
100%{-webkit-transform: scale(1);transform: scale(1); background: rgba(0,0,0,0.3);}
}
.show_mark{
    -webkit-animation: showMark .2s ease-in-out forwards;
            animation: showMark .2s ease-in-out forwards;
}
.search_source_scroll{
    width: 100%;
    background: #ffffff;
    box-sizing: border-box;
    position: relative;
}
.search_list_no_check:last-child{
    border-bottom: none;
}
.search_list_no_check{
    font-size: 28rpx;
    font-weight: normal;
    font-stretch: normal;
    letter-spacing: 0rpx;
    color: #333333;
    margin-left: 30rpx;
    line-height: 90rpx;
    border-bottom: 1px solid #F5F5F5;
}
.search_list_is_check{
    color: #0084ff;
}
