.customs_analysis {
  overflow: hidden;
}
.customs_analysis .header {
  position: fixed;
  background-color: #fff;
  z-index: 100;
  width: 100%;
  top: 0;
}
.customs_analysis .header .title_view {
  height: calc(var(--status-bar-height) + 60rpx);
  width: 100%;
  background-color: #fff;
}
.customs_analysis .header .tabs {
  width: 236rpx;
  margin: 115rpx auto 31rpx;
}
.customs_analysis .header .tabs .tabs_title {
  font-family: PingFang-SC-Medium;
  font-size: 36rpx;
  color: #7d8184;
  vertical-align: center;
  margin-bottom: 19rpx;
  height: 54rpx;
  line-height: 54rpx;
}
.customs_analysis .header .tabs .tabs_title .tabs_title_active {
  font-family: PingFang-SC-Bold;
  color: #191f25;
  font-size: 48rpx;
  font-weight: bold;
}
.customs_analysis .header .tabs .mr {
  margin-right: 67rpx;
}
.customs_analysis .header .tabs .d_inline {
  display: inline-block;
  height: 60rpx;
  line-height: 60rpx;
  width: 118rpx;
  text-align: center;
}
.customs_analysis .header .tabs .dots_list {
  width: 69%;
  margin: 0 auto;
  height: 5rpx;
}
.customs_analysis .header .tabs .dots_list .tabs_dots_left {
  width: 43rpx;
  height: 5rpx;
  border-radius: 3rpx;
  float: left;
}
.customs_analysis .header .tabs .dots_list .tabs_dots_right {
  width: 43rpx;
  height: 5rpx;
  border-radius: 3rpx;
  float: right;
}
.customs_analysis .header .tabs .dots_list .dot_active {
  background-color: #0084ff;
}
.customs_analysis .header .border_borttom {
  height: 1rpx;
  width: 100%;
  position: absolute;
  bottom: 0;
  background-color: #ebebeb;
}
.customs_analysis .main {
  margin-top: 210rpx;
}
.clearfix::after {
  display: block ;
  content: "";
  clear: both;
}

