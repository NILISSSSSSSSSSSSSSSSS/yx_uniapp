
page{
    width: 100%;
    height: 100%;
}
.list{
    width: 100%;
    height: 100%;
}
.scroll_view{
    width: 100%;
    height: 100%;
}
.scroll_cont{
    padding: 0 40rpx;
    box-sizing: border-box;
}
.list_item{
    box-sizing: border-box;
    width: 100%;
}
.list_right{
    width: 100%;
    padding: 30rpx 0 0;
    height: 152rpx;
    box-sizing: border-box;
}
.avatar_view{
    width: 94rpx;
    height: 94rpx;
    border-radius: 94rpx;
    overflow: hidden;
    -webkit-flex-shrink: 0;
            flex-shrink: 0;
    margin: 0 30rpx auto 0;
}
.avatar{
    width: 100%;
    height: 100%;
}
.list_right_info{
    height: 100%;
    border-bottom: 1rpx solid #ebebeb;
    box-sizing: border-box;
    width: 100%;
}
.consultantName{
    font-size: 32rpx;
    color: #191f25;
    line-height: 32rpx;
    margin-top: 10rpx;
    -webkit-align-content: center;
            align-content: center;
}
.consultant_status{
    height: 32rpx;
    line-height: 32rpx;
    border-radius: 4rpx;
    border: solid 1rpx rgb(125,129,132,0.2);
    margin-left: 10rpx;
    padding: 0 8rpx;
    box-sizing: border-box;
    width: 85px;
    font-family: PingFang-SC-Medium;
    font-size: 22rpx;
    font-weight: normal;
    font-stretch: normal;
    color: #a3a5a8;
    text-align: center;
    position: relative;
    top: -2rpx;
}
.consultantMobile{
    font-size: 28rpx;
    color: #a3a5a8;
    margin-top: 20rpx;
    line-height: 28rpx;
}
.im_msg_icon{
    width: 70rpx;
    height: 70rpx;
    -webkit-flex-shrink: 0;
            flex-shrink: 0;
    margin: 16rpx 40rpx auto 0;
}
.mobile_icon{
    width: 70rpx;
    height: 70rpx;
    -webkit-flex-shrink: 0;
            flex-shrink: 0;
    margin: 16rpx 0 auto 0;
}
.empty_view{
    height: 30rpx;
    -webkit-flex-shrink: 0;
            flex-shrink: 0;
    width: 100%;
}
.no_list_view{
    -webkit-box-flex: 1;
    -webkit-flex: 1;
            flex: 1;
    background: #ffffff;
    height: 100%;
}
.no_list_view>image{
    width: 374rpx;
    height: 168rpx;
    margin: -100rpx auto 0;
}
.no_list_view>view{
    text-align: center;
    line-height: 30rpx;
    margin-top: 50rpx;
    font-size: 30rpx;
    color: #c0c1c3;
}
.no_more_data {
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    font-size: 26rpx;
    color: #666666;
}

